/* pages/cal/cal.wxss */
.section_main{
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    background-color: #181C27;
   /* background-image: url("../res/img/66.png"); */
    background-repeat: repeat;
    padding-top:30rpx;
}
.text1{
  background-color: #FFFFFF;
  border-radius:3px;
  text-align: right;
  width: 720rpx; 
  height: 100rpx;
  line-height: 100rpx;
  padding-right:10rpx;
  margin-bottom:30rpx;  
}
.calnum{
  display: flex;
  flex-direction: row;
   
}
.num{
  width: 160rpx; 
  height: 150rpx;
  margin:10rpx;
  text-shadow: 0 1px 1px rgba(0,0,0,0.3);
  border-radius:5px;
  text-align: center;
  line-height: 150rpx;
}
.orange{
   color: #fef4e9;
   border: solid 1px #da7c0c;
   background:  #f78d1d;
}
.blue{
   color: #d9eef7;
   border: solid 1px #0076a3;
   background:  #0095cd;
}